
<h1>Listar pagamento</h1>
<div style="background-color: darkseagreen;height 400px;padding:13px;">
{{#each pagamentos}}
    ID : {{id}}<br>
    Nome : {{nome}}<br>
    valor : {{valor}}<br>
    Cadastrado : {{#formatDate createdAt}}{{/formatDate}}<br>
    <a href="/visu-pagamento/{{id}}"><button>Visualizar</button></a>
    <a href="/del-pagamento/{{id}}"><button>Apagar</button></a> 
    <a href="/cad-add-pagamento"><button>Cadastrar</button></a> 
    <a href="/editar-pagamento/{{id}}"><button>Editar</button></a> 
    <hr>
{{/each}}

</div>

